MainScreen:
    name: 'entry'

    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        Label:
            text: 'Do we want it?'
            font_size: 40
        Button:
            id: _button
            text: 'Start now'
            size_hint: 0.8, None
            height: 50
            pos_hint: {'center_x':.5}

            on_press: app.root.go_screen('search', 'left')
